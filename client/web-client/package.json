    "@material-ui/styles": "^4.11.3",
    "@material-ui/x-grid": "^4.0.0-alpha.20",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "chart.js": "^2.9.4",
    "d3-scale": "^3.2.3",
    "date-fns": "^2.17.0",
    "env-cmd": "^10.1.0",
    "node-sass": "^4.14.1",
